  var vIsIEHack = false;

  if( !isNull(pCheckBrowser) && pCheckBrowser == true )
  {
    vIsIEHack = getBrowser().substr(0,2).indexOf('IE') == -1;
  }

  if(!vIsIEHack)
  {
