    if(vType && (vType.toLowerCase() == "button" || vType.toLowerCase() == "submit"))
    {
      vResult[ pElement.name ] = pElement.value;
    }
  }

  return  ( !isNull(pGetHash) && !pGetHash )? Object.toQueryString(vResult) : vResult;
},

